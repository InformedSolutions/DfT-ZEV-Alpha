{
  "AllowedHostnames": "*",
  "BuildId": "",
  "DataProtection": {
    "KeyPath": "./data-protection",
    "ApplicationName": "ManufacturerReview"
  },
  "AdditionalCspSourceValues": "'nonce-owned-assets' *.analytics.google.com *.googletagmanager.com *.google-analytics.com",
  "StaticFilesCachePragmaInHours": 24,
  "GoogleAnalytics": {
    "TrackingCode": ""
  },
  "Phase": "alpha",
  "PhaseBannerContent": "This is a new service â€“ your feedback will help us to improve it.",
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.*": "Warning"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Enrichers.CorrelationId", "Serilog.Enrichers.Environment"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
         "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{CorrelationId}] [{EnvironmentName}] [{Application}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "Application": "ManufacturerReviewPortal"
    }
  },
  "EnableAuditLogging": false,
  "ServiceName": "Zero Emission Vehicles (Manufacturer Review)",
  "FeedbackUrl": "https://informed.com",
  "UserPasswordManagementEnabled": true,
  "SslHosted": false,
  "BasicAuth": {
    "IsEnabled": false,
    "Users": [
      {
        "Username": "tester",
        "Password": "tester"
      }
    ]
  },
  "ExternalCachedResourceDurationInMinutes": "60",
  "ShowCorrelationIdInErrorPages": true,
  "SessionExpirationNotificationInSeconds": 60,
  "PasswordPolicyDescription": "Must be at least 12 characters long, include at least 1 upper case letter, a number and a special character",
  "Postgres": {
    "DbName": "zev-dev",
    "Host": "localhost",
    "MaxPoolSize": 10,
    "Port": 5432,
    "UseSsl": false,
    "User": "root",
    "Password": "root"
  },
  "GoogleCloud": {
    "ProjectId": "informed-zev",
    "ApiKey": "<GCP_API_KEY>",
    "Tenancy": {
      "Admin": "administration-orudw",
      "Manufacturers": "manufacturers-k624k"
    },
    "Token": {
      "Issuer": "https://securetoken.google.com/informed-zev",
      "Audience": "informed-zev"
    }
  },
  "Services": {
    "OrganisationApiBaseUrl": "https://localhost:7273"
  }
}
